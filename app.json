{
  "expo": {
    "name": "LightingGrabberApp",
    "slug": "lightinggrabberapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lightinggrabberapp",
      "buildNumber": "3",
      "icon": "./assets/icon120.png",
      "jsEngine": "jsc" 
    },
    "android": {
      "package": "com.lightinggrabberapp",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "jsEngine": "jsc" 
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "dda61317-0bbd-4253-8024-fdd608e12b1b"
      }
    },
    "runtimeVersion": "2.0.0"
  }
}
